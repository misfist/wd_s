{
	"version": 2,
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true
		},
		"shadow": {
			"defaultPresets": false,
			"presets": [
				{
					"name": "Primary 200",
					"slug": "primary-200",
					"shadow": "0 4px 8px 0 rgba(59, 40, 204, 0.1), 0 0 1px 0 rgba(59, 40, 204, 0.05)"
				},
				{
					"name": "Primary 300",
					"slug": "primary-300",
					"shadow": "0 4px 8px 0 rgba(59, 40, 204, 0.1), 0 0 1px 0 rgba(59, 40, 204, 0.05)"
				},
				{
					"name": "Primary 400",
					"slug": "primary-400",
					"shadow": "0 8px 16px 0 rgba(59, 40, 204, 0.1), 0 0 1px 0 rgba(59, 40, 204, 0.05)"
				},
				{
					"name": "Primary 500",
					"slug": "primary-500",
					"shadow": "0 16px 24px 0 rgba(59, 40, 204, 0.09), 0 0 1px 0 rgba(59, 40, 204, 0.05)"
				},
				{
					"name": "Primary 600",
					"slug": "primary-600",
					"shadow": "0 24px 40px 0 rgba(59, 40, 204, 0.08), 0 0 1px 0 rgba(59, 40, 204, 0.05)"
				}
			]
		},
		"typography": {
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "Inter, sans-serif",
					"name": "Headline",
					"slug": "headline",
					"fontFace": [
						{
							"fontFamily": "Inter",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./build/fonts/inter/Inter-Light.woff2"
							]
						},
						{
							"fontFamily": "Inter",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./build/fonts/inter/Inter-Regular.woff2"
							]
						},
						{
							"fontFamily": "Inter",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./build/fonts/inter/Inter-Bold.woff2"
							]
						},
						{
							"fontFamily": "Inter",
							"fontStyle": "normal",
							"fontWeight": "900",
							"src": [
								"file:./build/fonts/inter/Inter-Black.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "Oxygen, sans-serif",
					"name": "Body",
					"slug": "body",
					"fontFace": [
						{
							"fontFamily": "Oxygen",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./build/fonts/oxygen/oxygen-v15-latin-300.woff2"
							]
						},
						{
							"fontFamily": "Oxygen",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./build/fonts/oxygen/oxygen-v15-latin-regular.woff2"
							]
						},
						{
							"fontFamily": "Oxygen",
							"fontStyle": "normal",
							"fontWeight": "900",
							"src": [
								"file:./build/fonts/oxygen/oxygen-v15-latin-700.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "Oxygen Mono, sans-serif",
					"name": "Mono",
					"slug": "mono",
					"fontFace": [
						{
							"fontFamily": "Oxygen Mono",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./build/fonts/oxygen/oxygen-mono-v14-latin-regular.woff2"
							]
						}
					]
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"min": "0.75rem",
						"max": "0.875rem"
					},
					"size": "0.875",
					"slug": "small",
					"name": "Small"
				},
				{
					"fluid": {
						"min": "0.875rem",
						"max": "1rem"
					},
					"size": "1rem",
					"slug": "normal",
					"name": "Normal"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.125rem"
					},
					"size": "1.125rem",
					"slug": "medium",
					"name": "Medium"
				},
				{
					"fluid": {
						"min": "1.75rem",
						"max": "1.875rem"
					},
					"size": "1.75rem",
					"slug": "large",
					"name": "Large"
				},
				{
					"fluid": false,
					"size": "2.25rem",
					"slug": "x-large",
					"name": "X Large"
				},
				{
					"fluid": {
						"min": "2.25rem",
						"max": "4rem"
					},
					"size": "4rem",
					"slug": "xx-large",
					"name": "XX Large"
				}
			]
		},
		"color": {
			"background": true,
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultGradients": false,
			"defaultPalette": false,
			"link": true,
			"text": true,
			"duotone": [],
			"gradients": [],
			"palette": [
				{
					"color": "#425a5e",
					"slug": "primary-100",
					"name": "Primary 100"
				},
				{
					"color": "#2d3d40",
					"slug": "primary-200",
					"name": "Primary 200"
				},
				{
					"color": "#3f4040",
					"slug": "primary-300",
					"name": "Primary 300"
				},
				{
					"color": "#fff",
					"slug": "secondary-100",
					"name": "Secondary 100"
				},
				{
					"color": "#e3e4eb",
					"slug": "secondary-200",
					"name": "Secondary 200"
				},
				{
					"color": "#d3d3d3",
					"slug": "secondary-300",
					"name": "Secondary 300"
				},
				{
					"color": "#f48d60",
					"slug": "tertiary-100",
					"name": "Tertiary 100"
				},
				{
					"color": "#f47c48",
					"slug": "tertiary-200",
					"name": "Tertiary 200"
				},
				{
					"color": "#f3713c",
					"slug": "tertiary-300",
					"name": "Tertiary 300"
				},
				{
					"color": "#22C55E",
					"slug": "success",
					"name": "Success"
				},
				{
					"color": "#EAB308",
					"slug": "warning",
					"name": "Warning"
				},
				{
					"color": "#EF4444",
					"slug": "error",
					"name": "Error"
				}
			]
		},
		"layout": {
			"contentSize": "1024px",
			"wideSize": "1280px"
		}
	},

	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--body)",
			"fontSize": "var(--wp--preset--font-size--normal)"
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--50)",
			"padding": {
				"top": "0",
				"right": "var(--wp--preset--spacing--50)",
				"bottom": "0",
				"left": "var(--wp--preset--spacing--50)"
			}
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--black)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--headline)",
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--headline)",
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--headline)",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"button": {
				"border": {
					"radius": "10px",
					"style": "solid",
					"width": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--tertiary-300)",
					"text": "var(--wp--preset--color--white)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)",
					"lineHeight": "1.435"
				},
				"spacing": {
					"padding": {
						"top": "8px",
						"right": "16px",
						"bottom": "8px",
						"left": "16px"
					}
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--tertiary-200)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--tertiary-200)"
					}
				},
				":active": {
					"color": {
						"background": "var(--wp--preset--color--tertiary-100)"
					}
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--tertiary-300)"
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--tertiary-200)"
					}
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--tertiary-200)"
					}
				},
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--tertiary-200)"
					}
				}
			}
		},
		"blocks": {
			"core/post-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "bold"
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--30)",
						"bottom": "var(--wp--preset--spacing--50)"
					}
				}
			},
			"core/heading": {
				"typography": {
					"fontWeight": "bold"
				},
				"spacing": {
					"margin": {
						"top": "0.5em",
						"bottom": "0.25em"
					}
				},
				"elements": {
					"h1": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--xx-large)"
						}
					},
					"h2": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--x-large)"
						}
					},
					"h3": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--large)"
						}
					},
					"h4": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--medium)"
						}
					},
					"h5": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--normal)"
						}
					},
					"h6": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--normal)"
						}
					}
				}
			},
			"core/paragraph": {
				"typography": {
					"lineHeight": "1.5"
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--20)",
						"bottom": "var(--wp--preset--spacing--20)"
					},
					"padding": {
						"bottom": "var(--wp--preset--spacing--20)"
					}
				}
			},
			"core/button": {
				"variations": {
					"outline": {
						"border": {
							"color": "currentColor",
							"style": "solid",
							"width": "1px"
						},
						"spacing": {
							"padding": {
								"top": "7px",
								"right": "15px",
								"bottom": "7px",
								"left": "15px"
							}
						}
					}
				}
			},
			"core/code": {
				"border": {
					"color": "var(--wp--preset--color--secondary-200)",
					"radius": "0.25rem",
					"style": "solid",
					"width": "2px"
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--50)",
						"left": "var(--wp--preset--spacing--50)",
						"right": "var(--wp--preset--spacing--50)",
						"top": "var(--wp--preset--spacing--50)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--mono)"
				}
			},
			"core/comment-author-name": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--tertiary-300)"
						},
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontStyle": "normal",
					"fontWeight": "600"
				}
			},
			"core/comment-content": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--20)",
						"bottom": "var(--wp--preset--spacing--20)"
					}
				}
			},
			"core/comment-date": {
				"color": {
					"text": "var(--wp--preset--color--primary-200)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary-200)"
						},
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"spacing": {
					"margin": {
						"top": "0px",
						"bottom": "0px"
					}
				}
			},
			"core/comment-edit-link": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary-200)"
						},
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-reply-link": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary-200)"
						},
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-comments-form": {
				"css": "& textarea, input{border-radius:.33rem}"
			},
			"core/comments-pagination": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comments-pagination-next": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comments-pagination-numbers": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comments-pagination-previous": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/image": {
				"css": "&.aligncenter {margin-left: auto; margin-right: auto;}"
			},
			"core/navigation": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-template": {
				"css": "& .wp-block-post:not(:last-of-type) {margin-bottom: var(--wp--preset--spacing--medium);}"
			},
			"core/preformatted": {
				"border": {
					"color": "var(--wp--preset--color--secondary-200)",
					"radius": "0.25rem",
					"style": "solid",
					"width": "2px"
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--50)",
						"left": "var(--wp--preset--spacing--50)",
						"right": "var(--wp--preset--spacing--50)",
						"top": "var(--wp--preset--spacing--50)"
					}
				},
				"color": {
					"background": "var(--wp--preset--color--secondary-100)",
					"text": "var(--wp--preset--color--black)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--mono)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/pullquote": {
				"border": {
					"bottom": {
						"style": "none",
						"width": "0px"
					},
					"left": {
						"style": "none",
						"width": "0px"
					},
					"radius": "var(--wp--preset--spacing--20)",
					"right": {
						"style": "none",
						"width": "0px"
					},
					"top": {
						"style": "none",
						"width": "0px"
					}
				},
				"elements": {
					"cite": {
						"typography": {
							"fontFamily": "var(--wp--preset--font-family--body)",
							"fontSize": "var(--wp--preset--font-size--small)",
							"fontStyle": "normal"
						}
					}
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--40)",
						"top": "var(--wp--preset--spacing--40)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--headline)",
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontStyle": "italic",
					"fontWeight": "400",
					"letterSpacing": "0em",
					"lineHeight": "1.5"
				}
			},
			"core/quote": {
				"border": {
					"radius": "var(--wp--preset--spacing--20)"
				},
				"color": {
					"background": "var(--wp--preset--color--tertiary-100)"
				},
				"css": "& :where(p) {margin-block-start:0;margin-block-end:calc(var(--wp--preset--spacing--20) + 0.5rem);}",
				"elements": {
					"cite": {
						"typography": {
							"fontFamily": "var(--wp--preset--font-family--body)",
							"fontSize": "var(--wp--preset--font-size--small)",
							"fontStyle": "normal"
						}
					}
				},
				"spacing": {
					"padding": {
						"bottom": "calc(var(--wp--preset--spacing--30) + 0.75rem)",
						"left": "calc(var(--wp--preset--spacing--30) + 0.75rem)",
						"right": "calc(var(--wp--preset--spacing--30) + 0.75rem)",
						"top": "calc(var(--wp--preset--spacing--30) + 0.75rem)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--headline)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontStyle": "italic",
					"lineHeight": "1.3"
				}
			},
			"core/separator": {
				"border": {
					"color": "currentColor",
					"radius": "0",
					"style": "solid",
					"width": "1px 0 0"
				},
				"css": ":not(.is-style-wide):not(.is-style-dots) {width: 100px;}"
			},
			"core/site-title": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"core/social-links": {
				"css": ".is-style-outline .wp-block-social-link {background-color: transparent;border: 1px solid currentColor;}"
			},
			"core/spacer": {
				"spacing": {
					"margin": {
						"top": "0 !important"
					}
				}
			},
			"core/table": {
				"css": "th {border-color: var(--wp--preset--color--primary-300) !important;} & td {border-color: var(--wp--preset--color--primary-300) !important;}",
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/template-part": {
				"spacing": {
					"margin": {
						"top": "0px !important"
					}
				}
			}
		}
	}
}
